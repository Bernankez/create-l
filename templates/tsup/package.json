{
  "type": "module",
  "version": "0.0.0",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepack": "nr build",
    "build": "tsup --format esm,cjs --clean --dts",
    "dev": "tsup --format esm,cjs --watch",
    "start": "tsx src/index.ts",
    "preview": "node --watch bin/index.js",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "test:once": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . -f mo",
    "fix": "eslint . -f mo --fix"
  },
  "dependencies": {
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.2",
    "@antfu/ni": "^0.22.0",
    "@types/node": "^20.14.10",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "eslint-formatter-mo": "^1.2.0",
    "lint-staged": "^15.2.7",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^2.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "nlx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}