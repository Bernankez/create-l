{
  "name": "create-l",
  "type": "module",
  "version": "1.0.0-beta.4",
  "packageManager": "pnpm@8.15.6",
  "description": "TypeScript Library Scaffold.",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/create-l#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/create-l.git"
  },
  "bugs": "https://github.com/Bernankez/create-l/issues",
  "keywords": [
    "template",
    "typescript",
    "lib",
    "scaffold",
    "starter"
  ],
  "sideEffects": false,
  "bin": {
    "create-l": "./bin/index.js",
    "cl": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "template"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "dev": "tsx --watch src/cli.ts",
    "build": "unbuild",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . -f mo",
    "fix": "eslint . -f mo --fix"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "taze": "^0.13.3"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^1.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.2",
    "bumpp": "^9.4.0",
    "enquirer": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-formatter-mo": "^1.2.0",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "scule": "^1.3.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vite": "^5.2.7",
    "vitest": "^1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
