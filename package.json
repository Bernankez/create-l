{
  "name": "create-l",
  "type": "module",
  "version": "2.0.1",
  "packageManager": "bun@1.1.3",
  "description": "TypeScript Library Scaffold.",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/create-l#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/create-l.git"
  },
  "bugs": "https://github.com/Bernankez/create-l/issues",
  "keywords": [
    "template",
    "typescript",
    "lib",
    "scaffold",
    "starter"
  ],
  "sideEffects": false,
  "bin": {
    "create-l": "./bin/index.js",
    "cl": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepack": "nr build",
    "build": "nr clean && tsup --format esm,cjs --clean --dts",
    "dev": "tsup --format esm,cjs --watch",
    "preview": "cross-env NODE_ENV=development node --watch bin/index.js",
    "clean": "tsx scripts/clean.ts",
    "bump": "tsx scripts/bump.ts",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "test:once": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . -f mo",
    "fix": "eslint . -f mo --fix"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "enquirer": "^2.4.1",
    "pacote": "^17.0.6",
    "sort-package-json": "^2.10.0",
    "taze": "^0.13.3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@bernankez/eslint-config": "^1.2.1",
    "@bernankez/utils": "^0.6.2",
    "@types/fs-extra": "^11.0.4",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.12.7",
    "@types/pacote": "^11.1.8",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "bumpp": "^9.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "eslint-formatter-mo": "^1.2.0",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "micromatch": "^4.0.5",
    "scule": "^1.3.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "nlx lint-staged --cwd ."
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
