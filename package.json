{
  "name": "create-l",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.5",
  "description": "Simple scaffold to create library.",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/bernankez/create-l#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bernankez/create-l.git"
  },
  "bugs": "https://github.com/bernankez/create-l/issues",
  "keywords": [
    "template",
    "typescript",
    "lib",
    "scaffold",
    "starter"
  ],
  "bin": {
    "create-l": "index.js",
    "cl": "index.js"
  },
  "files": [
    "index.js",
    "dist",
    "template"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "tsx src/index.ts",
    "watch": "tsx --watch src/index.ts",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.0",
    "@bernankez/prompt": "^0.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.3.3",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "fs-extra": "^11.1.1",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
