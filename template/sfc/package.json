{
  "name": "package-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.6",
  "description": "",
  "author": {
    "name": "author-name",
    "email": "author-email",
    "url": "author-homepage"
  },
  "license": "MIT",
  "homepage": "https://github.com/github-username/project-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github-username/project-name.git"
  },
  "bugs": "https://github.com/github-username/project-name/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js",
      "import": "./dist/components/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "build": "unbuild",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.0",
    "@bernankez/stylelint-config": "^0.4.3",
    "@types/node": "^20.2.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.41.0",
    "happy-dom": "^9.20.3",
    "lint-staged": "^13.2.3",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.9.0",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
